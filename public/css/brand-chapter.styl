#brand-chapter
  font-family "PT Sans"
  .info
    padding 20px 60px 60px 60px
    h3
      font-weight bold
      font-size 30px
      display inline-block
    ul
      list-style-type none
      margin-right 60px
      &.social
        margin-top 30px
        display inline-block
        float right
      li
        float left
        margin-left 10px
        a
          font-size 12px
          color #929292
          text-decoration none
    p
      font-size 18px
      font-weight 100
      margin-top 20px
      font-family "source sans pro" "PT Sans"
      

